#include<stdio.h>
int main(){
    int arr[100],temp,i,j,n;
    printf("enter the  size");
    scanf("%d",&n);
    printf("enter the elements");
    for(i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    for(i=0;i<=n-2;i++){
        for(j=1;j<=n-1;j++){
            if(arr[i]>arr[j]){
                temp=arr[i];
                arr[i]=arr[j];
                arr[j]=temp;
            }
        }
    }
    
    for(i=0;i<n;i++){
        printf("%d",arr[i]);

}
}
